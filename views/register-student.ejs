                </ul>
            
			</div>
        </div>
        <!--**********************************
            Sidebar end
        ***********************************-->
		
		<!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">
            <!-- row -->
			<div class="container-fluid">
				<div class="form-head d-flex mb-3 align-items-start">
					<div class="mr-auto d-none d-lg-block">
						<h2 class="text-black font-w600 mb-0">Register New Student</h2>
						<p class="mb-0">Fill in the fields correctly to register student</p>

					</div>
				</div>
                <div class="row">

                	
					<div class="col-xl-12 col-lg-12">
                        <div class="card">
                          
                            <div class="card-body">
                                <div class="basic-form">


                                    <form method="POST" action="/register-student-api">
                                        <div class="form-row">
                                          
                                            <div class="form-group col-md-6">
                                                <label>Reg. Number</label>
                                                <input type="text" id="regno" class="form-control" name="regno"
                                                    placeholder="Student Registration Number" required>
                                            </div>

                                           
                                            <div class="form-group col-md-6">
                                                <label>Pin</label>
                                                <input value="<%= randomPIN %>" type="text" id="pin" class="form-control" name="pin" readonly>
                                            </div>
                                            


                                            <div class="form-group col-md-12">
                                                <label>Level</label>
                                                <select name="level" id="level" class="form-control form-lg">
                                                    <option value="ND 1">ND 1</option>
                                                    <option value="ND 2">ND 2</option>
                                                    <option value="HND 1">HND 1</option>
                                                    <option value="HND 2">HND 2</option>
                                                </select>
                                            </div>


                                          

                                            <br><br><br>

                                            <div class="form-group col-md-12">
                                                <button class="btn btn-secondary btn-sm btn-block btn-lg"
                                                    style="background-color: #115E38;box-shadow: none;border: none;"
                                                    >
                                                    Proceed</button>
                                            </div>



                                        </div>

                                    </form>
                                </div>

                            </div>
                        </div>
                    </div>
				
					
					</div>
            </div>
        </div>
        <!--**********************************
            Content body end
        ***********************************-->


    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->

    <!--**********************************
        Scripts
    ***********************************-->
    <!-- Required vendors -->
    <script src="admin/vendor/global/global.min.js"></script>
	<script src="admin/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
	<script src="admin/vendor/chart.js/Chart.bundle.min.js"></script>
    <script src="admin/js/custom.min.js"></script>
	<script src="admin/js/deznav-init.js"></script>
	
	<!-- Counter Up -->
    <script src="admin/vendor/waypoints/jquery.waypoints.min.js"></script>
    <script src="admin/vendor/jquery.counterup/jquery.counterup.min.js"></script>	
		
	<!-- Apex Chart -->
	<script src="admin/vendor/apexchart/apexchart.js"></script>	
	
	<!-- Chart piety plugin files -->
	<script src="admin/vendor/peity/jquery.peity.min.js"></script>
	
	<!-- Dashboard 1 -->
	<script src="admin/js/dashboard/dashboard-1.js"></script>
	
	
</body>
</html>