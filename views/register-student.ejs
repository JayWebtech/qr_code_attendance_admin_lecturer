                        </ul>
                    </div>
                </nav>
            </div>
        </div>
        <!--**********************************
            Header end ti-comment-alt
        ***********************************-->

        <!--**********************************
            Sidebar start
        ***********************************-->
        <div class="deznav">
            <div class="deznav-scroll">
				<ul class="metismenu" id="menu">
                    <li><a class="ai-icon active" href="/dashboard"  aria-expanded="false">
							<i class="flaticon-381-networking"></i>
							<span class="nav-text">Dashboard</span>
						</a>
                    </li>
					<li><a href="/register-student" class="ai-icon" aria-expanded="false">
						<i class="flaticon-381-file"></i>
						<span class="nav-text">Register Student</span>
					</a>
				</li>
					<li><a href="/view-students" class="ai-icon" aria-expanded="false">
							<i class="flaticon-381-user"></i>
							<span class="nav-text">View Students</span>
						</a>
					</li>

					<li><a href="/register-lecturer" class="ai-icon" aria-expanded="false">
						<i class="flaticon-381-user"></i>
						<span class="nav-text">Register Lecturer</span>
					</a>
				</li>

				<li><a href="/view-lecturer" class="ai-icon" aria-expanded="false">
					<i class="flaticon-381-user"></i>
					<span class="nav-text">View Lecturer</span>
				</a>
			</li>

					<li><a href="/register-course" class="ai-icon" aria-expanded="false">
						<i class="flaticon-381-user"></i>
						<span class="nav-text">Register Course</span>
					</a>
				</li>

				<li><a href="/view-course" class="ai-icon" aria-expanded="false">
					<i class="flaticon-381-user"></i>
					<span class="nav-text">View Course</span>
				</a>
			</li>

				

					<li><a href="/logout" class="ai-icon" aria-expanded="false">
