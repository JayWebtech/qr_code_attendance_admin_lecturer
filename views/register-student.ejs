<body>

    <% if (errorMessage) { %>
        <script>
            swal.fire({
             icon : 'error',
                type : 'error',
                title : 'error',
                text :  '<%= errorMessage %>',
                confirmButtonColor: '#115E38',
            });
      
    </script>
    <% } else if (successMessage) { %>
        <script>
            swal.fire({
             icon : 'success',
                type : 'success',
                title : 'Success',
                text : 'Operation performed successfully',
                confirmButtonColor: '#115E38',
            });
      
    </script>
    <% } %>

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->

    <!--**********************************
        Main wrapper start
    ***********************************-->
